<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NonEditableUserData">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="40dp"
            android:layout_height="37dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/emailSignUpPage5"
            app:layout_constraintEnd_toStartOf="@+id/emailSignUpPage5"
            app:layout_constraintTop_toTopOf="@+id/emailSignUpPage5"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/mail_logo" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/usernameSignUpPage5"
            app:layout_constraintEnd_toStartOf="@+id/usernameSignUpPage5"
            app:layout_constraintTop_toTopOf="@+id/usernameSignUpPage5"
            app:srcCompat="@drawable/person_logo" />

        <TextView
            android:id="@+id/usernameSignUpPage5"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_marginTop="108dp"
            android:autofillHints=""
            android:backgroundTint="@color/sysLightBlue"
            android:ems="10"
            android:fontFamily="@font/roboto"
            android:gravity="center"
            android:hint="@string/username"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/emailSignUpPage5"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_marginTop="250dp"
            android:autofillHints=""
            android:backgroundTint="@color/sysLightBlue"
            android:editable="false"
            android:ems="10"
            android:fontFamily="@font/roboto"
            android:gravity="center"
            android:hint="@string/email"
            android:inputType="textEmailAddress"
            android:textAlignment="center"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>